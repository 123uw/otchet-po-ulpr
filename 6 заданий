int m = 0; 
for (int i = 0; i <= c; i++){
if (a == 0){
break;}
else{
n = % 10; 
m = a / 10;}
if (n % 2 == 0){
k+= n;}
a = m
m; += 1;}
Console.WriteLine(k);




пятое:
using System; 
int sum = 0; 
while (int.TryParse(Console.ReadLine(), out int n)){
if (n == 0){
break;}
sum += n;}
Console.WriteLine($"Сумма: {sum}");


